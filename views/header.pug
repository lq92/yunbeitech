//- This is Header component
header
  .logo
    a(href='/index'): img(src='/images/header/logo.png')
  ul.nav
    - let navs = [{ text: '首页', href: '/index' }, { text: '产品', href: '/products' }, { text: '文档API', href: '/documentation' }, { text: '联系', href: '/contact' }]
    each nav in navs
      li: a(href= nav.href)= nav.text
  #login
    a.register(href= '/register') 注册
    a.login(href='/login') 登录
